<!-- ProjectServerConfiguration.vm: Start -->
<div class="section">
  <h1 class="title">
    <span class="title">Project Configuration</span>
  </h1>
  <div class="section-content">
    <table>
      <tr>
        <td class="label strong">Queue</td>
        <td class="label strong">Priority</td>
        <td class="label strong">Project</td>
        <td class="label strong">Category</td>
        <td class="label strong">Web URL</td>
      </tr>
      #foreach ($project in $projects)
        <tr>
          <td class="data">$project.Queue</td>
          <td class="data">$project.QueuePriority</td>
          <td class="data strong">$project.Name</td>
          <td class="data">$project.Category</td>
          <td class="data">$project.WebURL</td>
        </tr>
      #end
    </table>
  </div>
</div>
<!-- ProjectServerConfiguration.vm: End -->